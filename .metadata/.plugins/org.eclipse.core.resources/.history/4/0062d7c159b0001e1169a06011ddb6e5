package com.cybage;

class Abc {
	int val= 400;
	void show()
	{
		System.out.println("In the ABC class");
	}
}

class PQR extends Abc
{
	int val = 700;
	void show()
	{
		System.out.println("Value is :"+ super.val); 
	}
}

public class SuperKeywordExample  
{
	public static void main(String[] args) {
		
		PQR pqr = new PQR();
		pqr.show();
	}
}
