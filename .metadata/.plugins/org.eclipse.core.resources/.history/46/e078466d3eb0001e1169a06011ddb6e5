package com.cybage;

class Employee{
	int id;
	String name;
	String designation;
	float salary;
	
//	constructor -- defualt
	Employee(){
		System.out.println("Default constructor");
	}
	Employee(int a){
		System.out.println("Parameterised constructor"+a);
	}
}

public class Inheritance {
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Employee e = new Employee();
		

	}

}
